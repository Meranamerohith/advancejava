package com.restaurant.udipi;

import java.util.ArrayList;
import java.util.LinkedHashSet;
import java.util.List;
import java.util.Set;

public class RestaurantCore {
             
	   Set<String> order = new LinkedHashSet<String>();
	   List<ArrayList<String>> items = new ArrayList<ArrayList<String>>();
	public RestaurantCore(Set<String> order, List<ArrayList<String>> items) {
		super();
		this.order = order;
		this.items = items;
	}
	@Override
	public String toString() {
		return "RestaurantCore [order=" + order + ", items=" + items + "]";
	}
	   
	   
	
}
